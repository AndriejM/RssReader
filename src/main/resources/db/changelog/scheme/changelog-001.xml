<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd"
    logicalFilePath="db/changelog/scheme/changelog-001.xml">

    <changeSet id="changeset-001-1" author="andrzej.mielnik">
        <createTable tableName="config">
            <column name="id" type="number(19,0)">
                <constraints primaryKey="true" primaryKeyName="PK_Config" />
            </column>
            <column name="user_id" type="number(19,0)"/>
            <column name="config_payload" type="TEXT(1024)" />
        </createTable>
    </changeSet>

</databaseChangeLog>
