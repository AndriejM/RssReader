<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
		http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.3.xsd"
                   logicalFilePath="db/changelog/scheme/changelog-001.xml">

    <changeSet id="changeset-010-1" author="andzrej.mielnik">
        <sql dbms="h2"
             endDelimiter=";"
             splitStatements="true"
             stripComments="true">
            INSERT INTO app_user (id, user_name) VALUES (NEXTVAL('global_sequence'), 'DEFAULT');
        </sql>
    </changeSet>

    <changeSet id="changeset-010-2" author="andzrej.mielnik">
        <sql dbms="h2"
             endDelimiter=";"
             splitStatements="true"
             stripComments="true">
            INSERT INTO config (id, user_id, config_payload)  select NEXTVAL('global_sequence'), select id from app_user where user_name = 'DEFAULT', '{"cronExpression": "0 */15 * ? * *"}' ;
        </sql>
    </changeSet>
</databaseChangeLog>
